{
  "eslint.validate": ["javascript", "javascriptreact"],
  "editor.formatOnPaste": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.fixAll": "always"
  },
  "editor.tabSize": 2,
  "typescript.suggest.autoImports": true,
  "[typescriptreact]": {
    "editor.formatOnSave": false
  },
  "git.autofetch": true
}
